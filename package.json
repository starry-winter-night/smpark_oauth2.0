    "prod": "cross-env NODE_ENV=production node ./dist/index.js",
    "build": "node build.mjs",
    "test": "jest && cypress run",
    "lint": "eslint .",
    "start:dev": "yarn docker:dev-build && yarn docker:dev-up",
    "start:prod": "yarn build && yarn docker:prod-build && yarn docker:prod-up",
    "docker:dev-build": "docker-compose -f docker-compose.dev.yml --env-file .env.development build",
    "docker:dev-up": "docker-compose -f docker-compose.dev.yml --env-file .env.development up",
    "docker:dev-down": "docker-compose -f docker-compose.dev.yml --env-file .env.development down",
    "docker:prod-build": "docker-compose -f docker-compose.yml --env-file .env.production build",
    "docker:prod-up": "docker-compose -f docker-compose.yml --env-file .env.production up -d",
